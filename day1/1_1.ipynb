            "source": [
                "with open(\"input1\") as input:\n",
                "  measurements = [int(line.strip()) for line in input.readlines()]\n",
                "\n",
                "increase_count = 0\n",
                "\n",
                "for i in range(len(measurements) - 1):\n",
                "  if measurements[i] < measurements[i+1]:\n",
                "    increase_count += 1\n",
                "\n",
                "print(f'Depth increased {increase_count} times.')\n",
                "\n"
            ]
        }
